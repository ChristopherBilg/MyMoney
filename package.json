{
  "name": "mymoney",
  "private": true,
  "version": "0.0.1",
  "description": "mymoney - your personal finance dashboard",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "api": "nodemon --watch src/api/ src/api/index.js",
    "build": "webpack --mode production",
    "serve": "http-server -a localhost -p 8080 dist/",
    "test": "nyc mocha src/**/*.test.js -r esm",
    "lint": "eslint src/",
    "prettier-check": "prettier src/ --check",
    "prettier-write": "prettier src/ --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "view-storybook": "http-server -a localhost -p 6006 storybook-static/"
  },
  "pre-commit": [
    "test",
    "lint",
    "prettier-check"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristopherBilg/mymoney.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChristopherBilg/mymoney/issues"
  },
  "homepage": "https://github.com/ChristopherBilg/mymoney#readme",
  "dependencies": {
    "@blueprintjs/core": "^3.28.1",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-persist": "^3.1.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "esm": "^3.2.25",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.0.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
